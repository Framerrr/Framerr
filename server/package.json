{
    "name": "framerr-server",
    "version": "0.1.4",
    "description": "Backend API server for Framerr",
    "main": "dist/index.js",
    "bin": {
        "framerr": "./dist/server/cli.js"
    },
    "scripts": {
        "start": "node dist/index.js",
