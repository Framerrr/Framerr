        "express": "^4.18.2",
        "express-rate-limit": "^7.5.1",
        "express-session": "^1.18.0",
        "fast-json-patch": "^3.1.1",
        "fuse.js": "^7.1.0",
        "helmet": "^7.1.0",
        "http-proxy-middleware": "^2.0.6",
        "ip-range-check": "^0.2.0",
